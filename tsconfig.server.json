{
	"compilerOptions": {
		"target": "es2020",
		"module": "commonjs",
		"strict": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"outDir": "./dist",
		"rootDir": "./server",
		"noImplicitAny": false
	},
	"include": ["server/**/*"],
	"exclude": ["node_modules"]
}
